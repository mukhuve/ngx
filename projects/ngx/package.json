{
  "name": "@mukhuve/ngx",
  "version": "0.0.10",
  "style": "./styles.scss",
  "exports": {
    ".": {
      "sass": "./styles.scss"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/ngx/"
